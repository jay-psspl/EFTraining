@model CheckBoxForEx.Models.CategoryList
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
@using (Html.BeginForm())
{
    for (int i = 0; i < Model.categorylist.Count; i++)
    {
        @Html.CheckBoxFor(x => Model.categorylist[i].isselected)
        @Html.HiddenFor(x => Model.categorylist[i].CategoryName)
        @Html.DisplayFor(x => Model.categorylist[i].CategoryName)

    }
    <input type="submit" name="name" value="Submit Data">
}
